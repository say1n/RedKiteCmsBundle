{
  "name": "redkite-cms/redkite-cms-bundle",
  "description": "RedKiteCms is an Open Source Content Management System Application, built on top of Symfony2 and TwitterBootstrap frameworks, providing an easy an intuitive user-interface, to improve the website's content management experience",
  "keywords": ["redkite", "redkite labs", "redkite cms", "cms"],
  "homepage": "http://redkite-labs.com",
  "type": "symfony-bundle",
  "license": "GPLv2",
  "authors": [
    {
      "name": "RedKite Labs",
      "email": "info@redkite-labs.com"
    }
  ],
  "repositories": [
      {
          "type": "composer",
          "url": "http://apps.redkite-labs.com/"
      }
  ],
  "require": {
    "php": ">=5.3.3",
    "imagine/Imagine": ">=0.4.1",
    "propel/propel-bundle": "1.2.*",
    "propel/propel1": "1.7.0",
    "redkite-labs/redkite-labs-bootstrap-bundle": "1.1.*",
    "redkite-labs/redkite-labs-theme-engine-bundle": "1.1.*",
    "redkite-labs/redkite-labs-elfinder-bundle": "1.1.*"
  },
  "require-dev": {    
    "symfony/symfony": "2.3.4",
    "doctrine/orm": ">=2.2.3,<2.4-dev",
    "doctrine/doctrine-bundle": "1.2.*",
    "twig/extensions": "1.0.*",
    "symfony/assetic-bundle": "2.3.*",
    "symfony/swiftmailer-bundle": "2.3.*",
    "symfony/monolog-bundle": "2.3.*",
    "sensio/distribution-bundle": "2.3.*",
    "sensio/framework-extra-bundle": "2.3.*",
    "sensio/generator-bundle": "2.3.*",
    "incenteev/composer-parameter-handler": "~2.0",
    "redkite-cms/redkite-cms-base-blocks": "1.1.*",
    "redkite-cms/tinymce-block-bundle": "1.1.*",
    "redkite-labs/bootbusiness-theme-bundle": "1.0.*",
    "redkite-labs/modern-business-theme-bundle": "1.0.*",
    "willdurand/propel-typehintable-behavior": "dev-master",  
    "mikey179/vfsStream": "1.1.0"
  },
  "autoload": {
    "psr-0": {
        "RedKiteLabs\\RedKiteCms\\RedKiteCmsBundle": ""
    }
  },
  "target-dir" : "RedKiteLabs/RedKiteCms/RedKiteCmsBundle"
}
